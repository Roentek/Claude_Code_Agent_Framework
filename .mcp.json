{
  "mcpServers": {
    "n8n-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "n8n-mcp"
      ],
      "env": {
        "MCP_MODE": "stdio",
        "LOG_LEVEL": "error",
        "DISABLE_CONSOLE_OUTPUT": "true",
        "N8N_API_URL": "${N8N_HOST_URL}",
        "N8N_API_KEY": "${N8N_API_KEY}"
      }
    },
    "tavily-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@latest"
      ],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}",
        "DEFAULT_PARAMETERS": "{\"include_images\": true, \"max_results\": 15, \"search_depth\": \"advanced\"}"
      }
    },
    "vapi-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@vapi-ai/mcp-server"
      ],
      "env": {
        "VAPI_TOKEN": "${VAPI_API_TOKEN}"
      }
    },
    "zep-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://docs-mcp.getzep.com/mcp"
      ]
    },
    "pinecone-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@pinecone-database/mcp"
      ],
      "env": {
        "PINECONE_API_KEY": "${PINECONE_API_KEY}"
      }
    },
    "apify-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@apify/mcp-server"
      ],
      "env": {
        "APIFY_TOKEN": "${APIFY_API_PAT}"
      }
    },
    "supabase-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_API_PAT}"
      }
    }
  }
}
